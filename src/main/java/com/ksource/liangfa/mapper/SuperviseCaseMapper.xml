  <sql id="superviseCase">
  	<where>
  		<if test="typeId != null">
  			AND SC.TYPE_ID = #{typeId }
  		</if>
  		<if test="caseId != null">
  			AND SC.CASE_ID LIKE CONCAT(CONCAT('%',#{caseId }),'%')
  		</if>
  		<if test="orgId != null">
  			AND SC.ORG_ID IN (SELECT O.ORG_CODE FROM ORGANISE O
         WHERE O.IS_DEPT = 0 START WITH O.ORG_CODE = #{orgId}
        CONNECT BY PRIOR O.ORG_CODE = O.UP_ORG_CODE) 
  		</if>
  	</where>
  </sql>
