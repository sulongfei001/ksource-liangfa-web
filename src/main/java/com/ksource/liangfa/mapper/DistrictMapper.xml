  <select id="find" parameterType="string" resultMap="BaseResultMap">
      select  t.*,
      (select count(*) from district k where k.up_district_code=t.district_code) as chirdNum
        from   district t  where   t.up_district_code =#{districtId}
        <if test="hasTopDis">
             or district_code=#{districtId}
        </if>
        order by SERIAL,DISTRICT_CODE
  </select>
  <select id="loadChildAsync" parameterType="com.ksource.liangfa.domain.District" resultMap="BaseResultMap">
      SELECT  T.*,
      (select count(*) from district k where k.up_district_code=t.district_code) as chirdNum
        FROM   DISTRICT T 
       		<where>
				<if test="upDistrictCode == null">
					 district_code=#{districtCode}
			    </if>
				<if test="upDistrictCode != null">
					 T.UP_DISTRICT_CODE =#{districtCode}
			    </if>
			</where>
        ORDER BY SERIAL,DISTRICT_CODE
  </select>
