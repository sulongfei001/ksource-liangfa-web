  <select id="queryByParentId" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT DISTRICT_CODE,
		         DISTRICT_NAME || '(' || DISTRICT_CODE || ')' AS DISTRICT_NAME,
		         UP_DISTRICT_CODE,
		         JB
		    FROM DISTRICT
		   WHERE 1=1
		   <if test="districtCode!=null">and UP_DISTRICT_CODE = #{districtCode}</if>
		ORDER BY DISTRICT_CODE  
	</select>
	
