  <select id="districtTreeManage" parameterType="map" resultMap="DistrictResultMap">
  	SELECT T.DISTRICT_CODE,
	       T.DISTRICT_NAME,
	       T.UP_DISTRICT_CODE,
	       (SELECT COUNT(*)
	          FROM DISTRICT D
	         WHERE D.UP_DISTRICT_CODE = T.DISTRICT_CODE) chirdNum
	  FROM DISTRICT T
	  <where>
	  	<if test="districtCode != null">
	  		AND T.DISTRICT_CODE = #{districtCode}
	  	</if>
	  	<if test="upDistrictCode != null">
	  		AND T.UP_DISTRICT_CODE = #{upDistrictCode}
	  	</if>
	  	<if test="jb != null">
	  		AND T.JB = #{jb}
	  	</if>
	  </where>
	  ORDER BY T.SERIAL ASC
  </select>
  <sql id="districtSql">
  	<where>
  		<if test="upDistrictCode != null">
  			 D.UP_DISTRICT_CODE = #{upDistrictCode}
  		</if>
  		<if test="districtCode != null">
  			AND D.DISTRICT_CODE LIKE CONCAT(CONCAT('%',#{districtCode}),'%')
  		</if>
  		<if test="districtName != null">
  			AND D.DISTRICT_NAME LIKE CONCAT(CONCAT('%',#{districtName}),'%')
  		</if>
  	</where>
  </sql>
  <select id="getPaginationList" parameterType="map" resultMap="BaseResultMap">
  	SELECT D.*,(SELECT COUNT(*) FROM ORGANISE O WHERE O.DISTRICT_CODE = D.DISTRICT_CODE) AS orgNum 
  	FROM DISTRICT D 
  	<include refid="districtSql"/>
  	ORDER BY D.SERIAL ASC
  </select>
  <select id="getPaginationCount" parameterType="map" resultType="int">
  	SELECT COUNT(*) FROM DISTRICT D
  	<include refid="districtSql"/>
  </select>
