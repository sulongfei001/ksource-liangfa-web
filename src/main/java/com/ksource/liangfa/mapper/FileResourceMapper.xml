    <select id="getPaginationList" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT G.ORG_NAME as uploadOrgName,U.USER_NAME AS uploaderName,R.*
	  	   FROM FILE_RESOURCE R,ORGANISE G,USER_ U
	     <where>
	     	G.ORG_CODE=U.ORG_ID AND R.UPLOADER=U.USER_ID
	       <if test="uploader!=null">
     		   AND R.UPLOADER=#{uploader}
      	   </if>
      	   <if test="uploaderName!=null">
      	   		AND U.USER_NAME LIKE concat(concat('%',#{uploaderName}),'%')
      	   </if>
      	   <if test="fileName!= null">
     		   AND R.FILE_NAME like concat(concat('%',#{fileName}),'%')
      	   </if>
      	   <if test="uploadOrgId!=null">
      	   		AND R.UPLOADER IN (SELECT USER_ID FROM USER_ WHERE ORG_ID=#{uploadOrgId})
      	   </if>
      	   <!-- orgId是登录用户所在单位ID -->
      	   <if test="orgId!=null">
      	   		AND  R.FILE_ID IN (SELECT FILE_ID FROM FILE_ORG O WHERE O.ORG_ID=#{orgId} )
      	   </if>
      	    <if test="startTime != null">
			<![CDATA[
			   and  UPLOAD_TIME >= #{startTime,jdbcType=TIMESTAMP}
			 	]]>
			</if>
			
			<if test="endTime != null">
			 <![CDATA[
			     and UPLOAD_TIME  < (#{endTime,jdbcType=TIMESTAMP} +   interval   '1'  day)
			 	]]>
			</if>
      	   <!-- <![CDATA[
      	  	and C.ID in (select P.CASE_CONSULTATION_ID FROM PARTICIPANTS P WHERE P.USER_ID=#{inputer,jdbcType=VARCHAR})
      	  	]]> -->
      	  	</where>
      	   order by R.UPLOAD_TIME desc
  </select>
  
